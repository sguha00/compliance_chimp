<%= form_for(proof, role: "form") do |form| %>
  <%= form.text_field :note, placeholder: "Out of scope for this requirement" %>
  <br>
  <br>
  <%= link_to "Cancel", requirements_path, class: "btn btn-default cancel-proof-changes" %>
  <%= form.filepicker_field :filepicker_url, services: "COMPUTER", button_class: "btn btn-default" %>
  <%= form.submit "Submit", class: "btn btn-primary" %>
<% end %>
